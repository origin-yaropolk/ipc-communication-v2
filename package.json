{
	"name": "tradingview-desktop",
	"version": "1.0.0",
	"productName": "TradingView",
	"description": "TradingView",
	"author": {
		"name": "TradingView, Inc.",
		"email": "support@tradingview.com",
		"url": "https://www.tradingview.com/"
	},
	"main": "dist/index.js",
	"scripts": {
		"prepare": "rimraf node_modules/electron/dist && node node_modules/electron/install.js",
		"build:dev": "pnpm check_licenses && pnpm --filter @tradingview/desktop-locales build && webpack --config webpack.config.js --env mode=development --env watchChanges=true",
		"build:prod": "pnpm check_licenses && pnpm --filter @tradingview/desktop-locales build && webpack --config webpack.config.js --env mode=production",
		"cpp:setup": "node ./modules/native-build --do setup",
		"cpp:build:dev": "native-build --do=build -s ./src_native -b ./src_native/.build --debug",
		"cpp:build:prod": "native-build --do=build -s ./src_native -b ./src_native/.build --force-clean",
		"extract_debug:dev": "tvd-tools prepare-debug-symbols --mode development",
		"extract_debug:prod": "tvd-tools prepare-debug-symbols --mode production",
		"pack:win": "electron-packager dist --out ./out --icon resources/logo.ico --app-copyright=\"Copyright (C) 2022 TradingView, Inc. All rights reserved.\" --asar.unpack=*.node",
		"pack:mac": "electron-builder --config electron-builder.js --mac --universal --publish never",
		"pack:snap:devel": "electron-builder --config electron-builder.js --config.snap.grade=\"devel\" --config.snap.confinement=\"strict\" --linux snap --publish never",
		"pack:snap": "electron-builder --config electron-builder.js --config.snap.grade=\"stable\" --config.snap.confinement=\"strict\" --linux snap --publish never",
		"pack:deb": "electron-builder --config electron-builder.js --linux deb --publish never",
		"pack:rpm": "electron-builder --config electron-builder.js --linux rpm --publish never",
		"upload_symbols": "sentry-cli upload-dif -o tradingview-6j -p tradingview-desktop out",
		"sentry-releases": "sentry-cli releases -o tradingview-6j -p tradingview-desktop",
		"lint": "pnpm lint:ascii && pnpm lint:ec && pnpm lint:eslint",
		"lint:ascii": "tvd-tools ascii-check .",
		"lint:ec": "ec",
		"lint:eslint": "eslint --ext .ts,.tsx src/",
		"ver": "tvd-tools version",
		"check_licenses": "license-checker --json  --out tools/licenses.json && tvd-tools parse-licenses",
		"test:unit": "eslint --ext .ts tests/ && pnpm --filter @tradingview/desktop-locales build && jest -c jest.unit.config.js",
		"test_spectron": "jest -c jest.spectron.config.js",
		"test": "pnpm lint && pnpm test:unit"
	},
	"native-build": {
		"config": "tools/native-build.json",
		"toolsDir": "tools/build-tools"
	},
	"private": true,
	"workspaces": [],
	"keywords": [],
	"license": "MIT",
	"devDependencies": {
		"@sentry/cli": "~2.3.1",
		"@sentry/types": "~7.53.1",
		"@svgr/webpack": "~6.1.0",
		"@tradingview/desktop-tools": "workspace:*",
		"@tradingview/tvd-native-build": "~0.0.7",
		"@types/archiver": "~5.1.1",
		"@types/jest": "~29.0.3",
		"@types/js-yaml": "~4.0.5",
		"@types/mocha": "^8.2.2",
		"@types/react": "~18.2.6",
		"@types/react-dom": "~18.2.4",
		"@types/react-window": "~1.8.5",
		"@types/semver": "~7.5.2",
		"@types/xml2js": "~0.4.9",
		"@typescript-eslint/eslint-plugin": "~5.59.5",
		"@typescript-eslint/parser": "~5.59.5",
		"acorn-jsx": "~5.3.2",
		"copy-webpack-plugin": "^9.0.1",
		"css-loader": "~6.7.4",
		"editorconfig-checker": "~4.0.2",
		"electron": "~29.1.5",
		"electron-builder": "~23.0.9",
		"electron-packager": "15.0.0",
		"eslint": "~8.56.0",
		"eslint-config-standard": "~17.1.0",
		"eslint-plugin-import": "~2.26.0",
		"eslint-plugin-react": "~7.30.0",
		"eslint-plugin-react-hooks": "~4.6.0",
		"eslint-plugin-simple-import-sort": "~10.0.0",
		"generate-json-webpack-plugin": "^2.0.0",
		"html-webpack-plugin": "~5.5.0",
		"jest": "~29.0.3",
		"license-checker": "^25.0.1",
		"native-ext-loader": "~2.3.0",
		"rimraf": "~5.0.5",
		"spectron": "~15.0.0",
		"style-loader": "~3.3.3",
		"terser-webpack-plugin": "~5.3.1",
		"ts-jest": "~29.1.0",
		"ts-loader": "~9.3.0",
		"typescript": "~5.0.4",
		"webpack": "~5.64.4",
		"webpack-cli": "~4.9.1",
		"webpack-merge": "~5.8.0",
		"webpack-preprocessor-loader": "~1.3.0"
	},
	"dependencies": {
		"@electron/remote": "~2.0.11",
		"@sentry/electron": "~4.6.0",
		"@sentry/integrations": "~7.53.1",
		"@tradingview/core": "~0.1.9",
		"@tradingview/ui-lib": "~1.2.1",
		"archiver": "~5.3.0",
		"electron-context-menu": "~3.6.1",
		"electron-updater": "~6.1.1",
		"i18next": "~22.4.15",
		"i18next-browser-languagedetector": "~7.1.0",
		"js-yaml": "~4.1.0",
		"keytar": "~7.9.0",
		"macos-notification-state": "2.0.1",
		"merge-anything": "~4.0.1",
		"react": "~18.2.0",
		"react-dom": "~18.2.0",
		"react-draggable": "~4.4.5",
		"react-i18next": "~12.2.2",
		"react-window": "~1.8.9",
		"reflect-metadata": "^0.1.13",
		"rxjs": "~7.8.1",
		"semver": "~7.5.2",
		"systeminformation": "~5.21.7",
		"uuidv4": "6.0.8",
		"xml2js": "~0.4.23"
	},
	"pnpm": {
		"patchedDependencies": {
			"app-builder-lib@23.0.9": "patches/app-builder-lib+23.0.9.patch",
			"keytar@7.9.0": "patches/keytar+7.9.0.patch",
			"macos-notification-state@2.0.1": "patches/macos-notification-state+2.0.1.patch",
			"@tradingview/tvd-native-build@0.0.7": "patches/@tradingview__tvd-native-build@0.0.7.patch"
		}
	},
	"volta": {
		"node": "18.17.1",
		"pnpm": "8.11.0"
	}
}
